#include <stdio.h>
int main()
 {
    int width, height;
    printf("Enter width of the box:");
    scanf("\n%d",&width);
    printf("Enter height of the box:");
    scanf("\n%d",&height);
    for (int i = 0; i < height; i++)
        {
        for (int j = 0; j < width; j++)
        {
            printf("*");
        }
        printf("\n");
    }
}
